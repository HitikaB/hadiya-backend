{
    "family": "hadiya-project",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "hadiya",
            "image": "318988877498.dkr.ecr.ap-south-1.amazonaws.com/hadiya-backend-hitika:latest",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}
     
