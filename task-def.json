
 {
        "family": "hadiya",
        "networkMode": "awsvpc",
        "taskRoleArn": "arn:aws:iam::318988877498:role/ecs-task-execution-role", 
        "containerDefinitions": [
            {
                "name": "hadiya",
                "image": "318988877498.dkr.ecr.ap-south-1.amazonaws.com/hadiya-backend-hitika:latest",
                "portMappings": [
                    {
                        "containerPort": 3000,
                        "hostPort": 3000,
                        "protocol": "tcp"
                    }
                ],
                "essential": true,
                "entryPoint": [
                    "sh",
                    "-c"
                ],
                "command": [
                "--config=/etc/ecs/ecs-cloudwatch.yaml" ],
            }
        ],
        "requiresCompatibilities": [
            "FARGATE"
        ],
        "cpu": "1024",
        "memory": "2048"
}
